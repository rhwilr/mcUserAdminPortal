{
	"name": "rhwilr/mcUserAdminPortal",
	"description": "",
	"keywords": ["laravel"],
	"license": "MIT",
	"type": "project",
	"repositories": [
	  {
		"type": "vcs",
		"url": "https://github.com/BootstrapCMS/Asset"
	  },
	  {
		"type": "vcs",
		"url": "https://github.com/BootstrapCMS/Sentry"
	  }
	],
	"require": {
		"laravel/framework": "5.0.*",
	    "illuminate/html": "5.0.*",
	  	"graham-campbell/credentials": "0.4.*@dev",
	  	"mccool/laravel-auto-presenter": "~3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "~4.0",
		"phpspec/phpspec": "~2.1"
	},
	"autoload": {
		"classmap": [
			"database"
		],
		"psr-4": {
			"rhwilr\\mcUserAdminPortal\\": "app/"
		}
	},
	"autoload-dev": {
		"classmap": [
			"tests/TestCase.php"
		]
	},
	"scripts": {
		"post-install-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-update-cmd": [
			"php artisan clear-compiled",
			"php artisan optimize"
		],
		"post-create-project-cmd": [
			"php -r \"copy('.env.example', '.env');\"",
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	}
}
